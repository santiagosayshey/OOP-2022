1-1: Capybara.cpp main-1-1.cpp
	g++ Capybara.cpp main-1-1.cpp -o 1-1.out
	./1-1.out

1-2: Person.cpp main-1-2.cpp
	g++ Person.cpp main-1-2.cpp -o 1-2.out
	./1-2.out

2-1: Capybara.cpp Wagon.cpp main-2-1.cpp
	g++ Capybara.cpp Wagon.cpp main-2-1.cpp -o 2-1.out
	./2-1.out

2-2: Person.cpp Airplane.cpp main-2-2.cpp
	g++ Person.cpp Airplane.cpp main-2-2.cpp -o 2-2.out
